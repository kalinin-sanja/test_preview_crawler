[MESSAGES CONTROL]
disable=missing-docstring,logging-fstring-interpolation,too-few-public-methods,no-member,too-many-arguments,too-many-locals

[MASTER]
init-hook="from pylint.config import find_pylintrc; import os, sys; sys.path.append(os.path.dirname(find_pylintrc()))"
ignore=migrations,venv,tests,.venv,data
extension-pkg-whitelist=pydantic

[FORMAT]
max-line-length=120
indent-string='    '
